{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "eslint-config-airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
  "rules": {
    "no-param-reassign": ["error", {"props": false}],
    "quotes": ["error", "single"],
    "no-iterator": [ "off" ],
    "no-return-await": [ "off" ]
    }
}
